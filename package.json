{
    "name": "webpack-focus",
    "version": "0.11.6",
    "description": "Focus webpack config",
    "main": "lib/index.js",
    "bin": {
        "focus-dev-server": "./lib/launcher.js"
    },
    "scripts": {
        "builder:gen-dev": "builder-support gen-dev",
        "build": "babel *.js --out-dir lib",
        "prepublish": "npm run build",
        "focus-dev-server": "focus-dev-server",
        "focus-webpack-bundle": "webpack --color --progress",
        "focus-webpack-watch": "webpack --color --progress --watch",
        "focus-webpack-profile": "webpack --progress --json --profile > profile.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KleeGroup/webpack-focus.git"
    },
    "keywords": [
        "focus",
        "webpack"
    ],
    "author": "focus@kleegroup.com",
    "license": "MIT",
    "homepage": "https://github.com/KleeGroup/webpack-focus#readme",
    "dependencies": {
        "autoprefixer": "6.6.1",
        "babel-cli": "6.22.2",
        "babel-core": "6.22.1",
        "babel-loader": "6.2.10",
        "babel-plugin-replace-require": "0.0.4",
        "babel-preset-focus": "git://github.com/KleeGroup/babel-preset-focus.git#refreshing-updating",
        "base64-font-loader": "0.0.4",
        "css-loader": "0.26.1",
        "eslint-config-focus": "git://github.com/KleeGroup/eslint-config-focus.git#updating-refreshing",
        "extract-text-webpack-plugin": "1.0.1",
        "file-loader": "0.9.0",
        "html-webpack-plugin": "2.26.0",
        "json-loader": "0.5.4",
        "lodash": "4.17.4",
        "node-sass": "4.3.0",
        "postcss-loader": "1.2.2",
        "react-hot-loader": "3.0.0-beta.6",
        "sass-loader": "4.1.1",
        "source-map-loader": "0.1.6",
        "style-loader": "0.13.1",
        "url-loader": "0.5.7",
        "webpack": "1.14.0",
        "webpack-alternate-require-loader": "0.0.2",
        "webpack-dev-server": "1.16.2"
    },
    "devDependencies": {
        "builder-support": "0.3.2"
    }
