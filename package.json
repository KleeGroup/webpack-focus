{
    "name": "webpack-focus",
    "version": "1.0.4",
    "description": "Focus webpack config",
    "main": "./index.js",
    "bin": {
        "focus-dev-server": "./launcher.js"
    },
    "scripts": {
        "prepublishOnly": "npm run build",
        "build": "better-npm-run build"
    },
    "babel": {
        "presets": [
            "focus"
        ]
    },
    "betterScripts": {
        "build": {
            "command": "babel src/ --out-dir .",
            "env": {
                "NODE_ENV": "development",
                "LEGACY_EXPORTS": "true"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KleeGroup/webpack-focus.git"
    },
    "keywords": [
        "focus",
        "webpack"
    ],
    "author": "focus@kleegroup.com",
    "license": "MIT",
    "homepage": "https://github.com/KleeGroup/webpack-focus#readme",
    "dependencies": {
        "autoprefixer": "8.1.0",
        "babel-loader": "7.1.4",
        "babel-polyfill": "6.26.0",
        "babel-preset-focus": "1.0.0",
        "cache-loader": "1.2.2",
        "case-sensitive-paths-webpack-plugin": "2.1.2",
        "css-loader": "0.28.10",
        "cssnano": "4.0.0-rc.2",
        "eslint-config-focus": "0.6.1",
        "eslint-loader": "2.0.0",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "1.1.11",
        "fork-ts-checker-webpack-plugin": "0.4.1",
        "html-webpack-plugin": "3.0.6",
        "lodash": "4.17.5",
        "node-sass": "4.8.1",
        "null-loader": "0.1.1",
        "postcss-cssnext": "3.1.0",
        "postcss-flexbugs-fixes": "3.3.0",
        "postcss-import": "11.1.0",
        "postcss-loader": "2.1.1",
        "react-dev-utils": "4.1.0",
        "react-error-overlay": "2.0.2",
        "react-hot-loader": "4.0.0",
        "sass-loader": "6.0.7",
        "source-map-loader": "0.2.3",
        "style-loader": "0.20.3",
        "thread-loader": "1.1.5",
        "ts-loader": "4.0.1",
        "uglifyjs-webpack-plugin": "1.2.3",
        "url-loader": "1.0.1",
        "webpack": "3.11.0",
        "webpack-bundle-analyzer": "2.11.1",
        "webpack-dev-server": "2.11.1",
        "whatwg-fetch": "2.0.3"
    },
    "devDependencies": {
        "babel-cli": "6.26.0",
        "better-npm-run": "0.1.0"
    }
}
